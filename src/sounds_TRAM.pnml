    return 1;
}

switch(FEAT_ROADVEHS,SELF,sw_sound_TRAM_default,getbits(extra_callback_info1, 0, 8)){
	SOUND_EVENT_START: 			SOUND_TRAIN_HORN;
	SOUND_EVENT_BREAKDOWN:		SOUND_TRAIN_BREAKDOWN;
}

switch(FEAT_ROADVEHS,SELF,sw_sound_TRAM_steam_default,getbits(extra_callback_info1, 0, 8)){
	SOUND_EVENT_TUNNEL:			SOUND_TRAIN_THROUGH_TUNNEL;
	SOUND_EVENT_START: 			SOUND_TRAIN;
	SOUND_EVENT_BREAKDOWN:		SOUND_TRAIN_BREAKDOWN;
}

/* 
	Class 08
	
	Use this for shunters.
	Only needs a running sound and not an exhaust effect sound as shunters sound consistent in motion.
*/

